  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.19.12",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5"
