  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "node ratemyrations/static/build/build.js",
    "dev": "node ratemyrations/static/build/dev.js",
    "build:prod": "NODE_ENV=production npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jharlan-hash/RateMyRations.git"
  },
